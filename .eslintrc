{
	"extends": "eslint:recommended",
	"plugins": [
		"angular"
	],
	"env": {
		"browser": true,
		"jasmine": true
	},
	"globals": {
		"angular": true,
		"module": true,
		"inject": true,
		"cordova": true,
		"StatusBar": true
	},
	"rules": {
		"angular/controller-name": [2, "/[A-Z].*Ctrl/"],
		"angular/controller-as": 0

	}
}
